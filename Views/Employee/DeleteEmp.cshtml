@model Employee
@{
    ViewData["Title"] = "DeleteEmp";
}

<h1>DeleteEmp</h1>
<h2>Are you sure?</h2>

<label class="fw-bold">@Model.Name</label>
<form method="post">
    <input type="hidden" name="Id" value="@Model.Id" />
    <input type="submit" value="Confirm" class="btn btn-danger" />
</form>

<a href="/Employee/Index" class="btn btn-primary">Cancel</a>

