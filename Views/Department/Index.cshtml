@model List<Department>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Manager</th>
        
    </tr>
    @foreach (var dept in Model)
    {
        <tr>
            <td>@dept.DeptID</td>
            <td>@dept.Name</td>
            <td>@dept.ManagerName</td>
           
        </tr>
    }
</table>

<a href="Department/NewDept" class="btn btn-success">New</a>

